CREATE TABLE IF NOT EXISTS SES (CLIENT_NAME TEXT, IP TEXT, DT DATE)
CREATE TABLE IF NOT EXISTS DATA (ID INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, DT DATE, DATA TEXT)
CREATE TABLE IF NOT EXISTS LOG (ID INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, DT DATE, LOG_SYS TEXT)
CREATE TABLE IF NOT EXISTS Relays ("Relay"  TEXT(10) NOT NULL,"Position"  TEXT(3) NOT NULL)
CREATE TABLE "INPUT_PHOTOS" ("ID_USER"  INTEGER NOT NULL,"DATA"  BLOB NOT NULL,"ID"  INTEGER NOT NULL,"TIME_ADD"  DATE,"FIX"  INTEGER,PRIMARY KEY ("ID"))
CREATE TABLE "USERS" ("ID"  INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,"NAME"  TEXT(30) NOT NULL)
